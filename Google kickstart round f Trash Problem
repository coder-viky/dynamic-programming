#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    int cased = 1;
    while(t--)
    {
        int n;
        cin >> n;
        string trash;
        cin >> trash;
        int leftdistance[n] = {0};
        int rightdistance[n]={0};
        int count = 0;
        int index = 0;
        while(trash[index]!='1')
        {
            leftdistance[index] = INT_MAX;
            index++;
        }
        for (int i = index;i<n;i++)
        {
            if(trash[i]=='1')
            {
                leftdistance[i] = count;
                count = 0;
            }
            else
            {
                count++;
                leftdistance[i] = count;
            }
        }
        count = 0;
        index = n-1;
        while(trash[index]!='1')
        {
            rightdistance[index] = INT_MAX;
            index--;
        }
        for (int i = index;i>=0;i--)
        {
            if(trash[i]=='1')
            {
                rightdistance[i] = count;
                count = 0;
            }
            else
            {
                count++;
                rightdistance[i] = count;
            }
        }
        long long int sum = 0;

        for (int i = 0; i < n;i++)
        {
            sum = sum + min(rightdistance[i],leftdistance[i]);
        }
        cout << "Case #"<<cased<<": "<<sum<<"\n";
        cased++;
    }
    return 0;
}
